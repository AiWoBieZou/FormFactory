@using System
@using System.ComponentModel.DataAnnotations
@using FormFactory
@using FormFactory.AspMvc
@model PropertyVm
@{
    var vm = new FormFactory.ViewHelpers.DateTimeOffsetVm(Model);
    <input type="text" class="ff-behaviour" data-ff-behaviour="@vm.behaviour" data-ff-format="@vm.stringFormat" name="@Model.Name" @Html.UnobtrusiveValidation(Model) value="@vm.valueAsString" @FormFactory.ViewHelper.Readonly(Model) @FormFactory.ViewHelper.Disabled(Model) />
}
